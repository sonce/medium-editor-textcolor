{
  "env": {
      "browser": true,
      "es6": true,
      "node": false
  },
  "extends": [
      "eslint:recommended"
  ],
  "parserOptions": {
      "sourceType": "module"
  },
  "rules": {
      "indent": [
          "error",
          4
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "no-console": "off",
      "max-classes-per-file": ["error", 2],
  }
}
